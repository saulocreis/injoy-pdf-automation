	<queryString>
		<![CDATA[SELECT
	(SELECT CONCAT($P{img_dir}, "carneiros2019", $P{sep}, t.arquivo)
		FROM pdf t WHERE t.slug IN ("jr_carneiros2019_ac_sitioprainha_precos")) 
	AS arquivo1,
    (SELECT ROUND(efd.valor) FROM experiencia_festadias efd
		WHERE efd.idProduto IN (1723) AND efd.id IN (777)) 
	AS festaMasculino,
	(SELECT ROUND(efd.valor) FROM experiencia_festadias efd
		WHERE efd.idProduto IN (1723) AND efd.id IN (776)) 
	AS festaFeminino,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes))
	FROM acomodacao_quarto aq INNER JOIN produto p ON aq.idProduto = p.id
	WHERE p.id IN (1713) AND aq.id IN (25101))
    as valorPessoa_1713_25101,
    (SELECT valorPessoa_1713_25101 + festaMasculino) 
    AS festaMasculino_1713_25101,
    (SELECT valorPessoa_1713_25101 + festaFeminino) 
    AS festaFeminino_1713_25101,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes))
	FROM acomodacao_quarto aq INNER JOIN produto p ON aq.idProduto = p.id
	WHERE p.id IN (1713) AND aq.id IN (25102))
    as valorPessoa_1713_25102,
    (SELECT valorPessoa_1713_25102 + festaMasculino) 
    AS festaMasculino_1713_25102,
    (SELECT valorPessoa_1713_25102 + festaFeminino) 
    AS festaFeminino_1713_25102,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes))
	FROM acomodacao_quarto aq INNER JOIN produto p ON aq.idProduto = p.id
	WHERE p.id IN (1713) AND aq.id IN (25103))
    as valorPessoa_1713_25103,
    (SELECT valorPessoa_1713_25103 + festaMasculino) 
    AS festaMasculino_1713_25103,
    (SELECT valorPessoa_1713_25103 + festaFeminino) 
    AS festaFeminino_1713_25103,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes))
	FROM acomodacao_quarto aq INNER JOIN produto p ON aq.idProduto = p.id
	WHERE p.id IN (1713) AND aq.id IN (25104))
    as valorPessoa_1713_25104,
    (SELECT valorPessoa_1713_25104 + festaMasculino) 
    AS festaMasculino_1713_25104,
    (SELECT valorPessoa_1713_25104 + festaFeminino) 
    AS festaFeminino_1713_25104,
    (SELECT ROUND(MIN((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes)))
	FROM acomodacao_quarto aq 
	WHERE aq.idProduto IN (1713)) 
	AS menorValor,
    (SELECT ROUND(aet.valor + aet.taxa) + menorValor
		FROM aereo_trecho aet
        WHERE aet.id IN (2231))
    AS pacoteMasculino,
    (SELECT ROUND(aet.valor + aet.taxa) + menorValor
		FROM aereo_trecho aet
        WHERE aet.id IN (2232))
	AS pacoteFeminino
	;]]>
