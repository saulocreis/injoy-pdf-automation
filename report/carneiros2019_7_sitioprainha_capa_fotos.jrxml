	<queryString>
		<![CDATA[SELECT
	(SELECT CONCAT("images\\carneiros2019\\", t.arquivo)
	FROM pdf t
	WHERE t.slug IN ("jr_carneiros2019_ac_sitioprainha_capa"))
    as arquivo1,
	(SELECT CONCAT("images\\carneiros2019\\", t.arquivo)
	FROM pdf t
	WHERE t.slug IN ("jr_carneiros2019_ac_sitioprainha_fotos1"))
    as arquivo2,
	(SELECT CONCAT("images\\carneiros2019\\", t.arquivo)
	FROM pdf t
	WHERE t.slug IN ("jr_carneiros2019_ac_sitioprainha_fotos2"))
    as arquivo3,
    (SELECT ROUND(MIN((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes)))
	FROM acomodacao_quarto aq 
	WHERE aq.idProduto IN (1713)) as menorValor;]]>
	</queryString>
