	<queryString>
		<![CDATA[SELECT p.id as idProduto, p.nome as nomeProduto,
		ROUND(MIN((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes)), 0) as menorValor
		FROM acomodacao_quarto aq INNER JOIN produto p ON aq.idProduto = p.id
		WHERE p.id IN (1716)
		;]]>
	</queryString>
