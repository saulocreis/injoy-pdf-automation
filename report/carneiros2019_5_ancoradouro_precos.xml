	<queryString>
		<![CDATA[SELECT 
	(SELECT CONCAT("images\\carneiros2019\\", t.arquivo) AS arquivo1 
		FROM pdf t WHERE t.slug IN ("jr_carneiros2019_ac_hotel-ancoradouro_precos")) as arquivo1,
    (SELECT ROUND(efd.valor) FROM experiencia_festadias efd
		WHERE efd.idProduto IN (1723) AND efd.id IN (777)) AS festaMasculino,
	(SELECT ROUND(efd.valor) FROM experiencia_festadias efd
		WHERE efd.idProduto IN (1723) AND efd.id IN (776)) AS festaFeminino,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25069)) 
    as valorPessoa_1744_25069,
    (SELECT valorPessoa_1744_25069 + festaMasculino) AS festaMasculino_1744_25069,
    (SELECT valorPessoa_1744_25069 + festaFeminino) AS festaFeminino_1744_25069,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25070)) 
    as valorPessoa_1744_25070,
    (SELECT valorPessoa_1744_25070 + festaMasculino) AS festaMasculino_1744_25070,
    (SELECT valorPessoa_1744_25070 + festaFeminino) AS festaFeminino_1744_25070,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25071)) 
    as valorPessoa_1744_25071,
    (SELECT valorPessoa_1744_25071 + festaMasculino) AS festaMasculino_1744_25071,
    (SELECT valorPessoa_1744_25071 + festaFeminino) AS festaFeminino_1744_25071,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25072)) 
    as valorPessoa_1744_25072,
    (SELECT valorPessoa_1744_25072 + festaMasculino) AS festaMasculino_1744_25072,
    (SELECT valorPessoa_1744_25072 + festaFeminino) AS festaFeminino_1744_25072,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25073)) 
    as valorPessoa_1744_25073,
    (SELECT valorPessoa_1744_25073 + festaMasculino) AS festaMasculino_1744_25073,
    (SELECT valorPessoa_1744_25073 + festaFeminino) AS festaFeminino_1744_25073,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25074)) 
    as valorPessoa_1744_25074,
    (SELECT valorPessoa_1744_25074 + festaMasculino) AS festaMasculino_1744_25074,
    (SELECT valorPessoa_1744_25074 + festaFeminino) AS festaFeminino_1744_25074,
	(SELECT ROUND((DATEDIFF(aq.data_final, aq.data_inicial) * aq.valor / aq.hospedes), 0)
	FROM acomodacao_quarto aq
	WHERE aq.idProduto IN (1744) AND aq.id IN (25075)) 
    as valorPessoa_1744_25075,
    (SELECT valorPessoa_1744_25075 + festaMasculino) AS festaMasculino_1744_25075,
